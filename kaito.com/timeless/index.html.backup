 <script>
 // Function untuk update Instagram link - support link lengkap atau username saja
 function updateInstagramLink(person, instagramValue) {
   // Cek apakah yang dimasukkan link lengkap atau username saja
   var instagramUrl;
   if (instagramValue && instagramValue.includes('instagram.com')) {
     // Jika sudah link lengkap, pakai apa adanya
     instagramUrl = instagramValue;
   } else if (instagramValue) {
     // Jika hanya username, buat link lengkap
     instagramUrl = 'https://instagram.com/' + instagramValue;
   } else {
     // Jika kosong, jangan update apa-apa
     return;
   }
   
   // Update link di elemen yang sesuai
   if (person === 'groom') {
     // Cari link Instagram di area groom (berdasarkan struktur HTML)
     var instagramLink = document.querySelector('#profile a[href*="instagram.com"]');
     if (instagramLink) {
       instagramLink.href = instagramUrl;
       // Update juga text di dalam span jika ada
       var instagramText = instagramLink.querySelector('.elementor-button-text');
       if (instagramText) {
         // Ambil username dari URL untuk ditampilkan
         var username = instagramUrl.split('instagram.com/').pop();
         // Hapus trailing slash jika ada
         username = username.replace(/\/$/, '');
         instagramText.textContent = username;
       }
     }
   } else if (person === 'bride') {
     // Cari link Instagram di area bride (berdasarkan struktur HTML)
     var brideSection = document.querySelector('.background-section-3');
     if (brideSection) {
       var instagramLink = brideSection.querySelector('a[href*="instagram.com"]');
       if (instagramLink) {
         instagramLink.href = instagramUrl;
         // Update juga text di dalam span jika ada
         var instagramText = instagramLink.querySelector('.elementor-button-text');
         if (instagramText) {
           // Ambil username dari URL untuk ditampilkan
           var username = instagramUrl.split('instagram.com/').pop();
           // Hapus trailing slash jika ada
           username = username.replace(/\/$/, '');
           instagramText.textContent = username;
         }
       }
     }
   }
 }
 </script>
 <script>
 // Load konten hero dari content.json dan isi elemen halaman
document.addEventListener('DOMContentLoaded', function () {
  // Fail-safe: make hero section visible after 2 seconds max, even if content.json fails
  setTimeout(function() {
    var heroSection = document.getElementById('hapus-section');
    if (heroSection && heroSection.style.opacity === '0') {
      heroSection.style.opacity = '1';
      heroSection.style.visibility = 'visible';
      // Force override AOS animations for hero section only
      heroSection.classList.add('aos-animate');
      heroSection.classList.remove('aos-init');
    }
  }, 2000);
  
  // Enhanced AOS initialization - wait for all images to load first
  window.addEventListener('load', function() {
    setTimeout(function() {
      if (typeof AOS !== 'undefined') {
        AOS.refresh();
        // Triple refresh for groom and bride sections
        setTimeout(function() {
          AOS.refresh();
          setTimeout(function() {
            AOS.refresh();
            // Final refresh after hidden classes are removed
            setTimeout(function() {
              AOS.refresh();
              // Also refresh event sections after all other refreshes
              setTimeout(function() {
                ensureAOSForEventSections();
              }, 300);
            }, 200);
          }, 1000);
        }, 500);
      }
    }, 300);
  });
   fetch('/data/content.json')
