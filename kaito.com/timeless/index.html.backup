         heroTitleEl.textContent = content.hero_title;
       }
       // Nama mempelai
       var brideNameEl = document.getElementById('bride-name');
       if (brideNameEl && content.bride_name) {
         brideNameEl.textContent = content.bride_name;
       }
       // Tanggal pernikahan (semua elemen dengan class wedding-date)
       if (content.wedding_date) {
         var weddingDateElements = document.querySelectorAll('.wedding-date');
         weddingDateElements.forEach(function(el) {
           el.textContent = content.wedding_date;
         });
       }
       // Lokasi pernikahan
       if (content.wedding_location) {
         var weddingLocationElements = document.querySelectorAll('.wedding-location');
         weddingLocationElements.forEach(function(el) {
           el.textContent = content.wedding_location;
         });
       }
       // Teks RSVP
       if (content.rsvp_text) {
         var rsvpTextElements = document.querySelectorAll('.rsvp-text');
         rsvpTextElements.forEach(function(el) {
           el.textContent = content.rsvp_text;
         });
       }
       // Nama groom lengkap
       if (content.groom_full_name) {
         var groomNameElements = document.querySelectorAll('.groom-full-name');
         groomNameElements.forEach(function(el) {
           el.textContent = content.groom_full_name;
         });
       }
       // Nama bride lengkap
       if (content.bride_full_name) {
         var brideNameElements = document.querySelectorAll('.bride-full-name');
         brideNameElements.forEach(function(el) {
           el.textContent = content.bride_full_name;
         });
       }
       // Orang tua groom
       if (content.groom_parents) {
         var groomParentsElements = document.querySelectorAll('.groom-parents');
         groomParentsElements.forEach(function(el) {
           el.textContent = content.groom_parents;
         });
       }
       // Deskripsi groom
       if (content.groom_description) {
         var groomDescElements = document.querySelectorAll('.groom-description');
         groomDescElements.forEach(function(el) {
           el.textContent = content.groom_description;
         });
       }
       // Instagram groom
       if (content.groom_instagram) {
         updateInstagramLink('groom', content.groom_instagram);
       }
       // Label bride
       if (content.bride_label) {
         var brideLabelElements = document.querySelectorAll('.bride-label');
         brideLabelElements.forEach(function(el) {
           el.textContent = content.bride_label;
         });
       }
       // Nama bride lengkap (backup)
       if (content.bride_full_name) {
         var brideNameElements = document.querySelectorAll('.bride-full-name');
         brideNameElements.forEach(function(el) {
           el.textContent = content.bride_full_name;
         });
       }
       // Orang tua bride
       if (content.bride_parents) {
         var brideParentsElements = document.querySelectorAll('.bride-parents');
         brideParentsElements.forEach(function(el) {
           el.textContent = content.bride_parents;
         });
       }
       // Deskripsi bride
       if (content.bride_description) {
         var brideDescElements = document.querySelectorAll('.bride-description');
         brideDescElements.forEach(function(el) {
           el.textContent = content.bride_description;
         });
       }
       // Instagram bride
       if (content.bride_instagram) {
         updateInstagramLink('bride', content.bride_instagram);
       }
       // Journey title
       if (content.journey_title) {
         var journeyElements = document.querySelectorAll('.journey-title');
         journeyElements.forEach(function(el) {
           el.textContent = content.journey_title;
         });
       }
       // Journey content
       if (content.journey_content) {
         // Update konten journey - cari element text-editor di area lovestory
         var journeyContentElements = document.querySelectorAll('#lovestory .elementor-widget-text-editor');
         if (journeyContentElements.length > 0 && content.journey_content) {
           journeyContentElements[0].innerHTML = content.journey_content.replace(/\n/g, '<br>');
         }
       }
       // Wedding date main
       if (content.wedding_date_main) {
         var weddingDateMainElements = document.querySelectorAll('#weddingevent h2.elementor-heading-title');
         if (weddingDateMainElements.length > 0) {
           weddingDateMainElements[0].innerHTML = content.wedding_date_main.replace(/\n/g, '<br>');
         }
       }
       // Holy matrimony details
       if (content.holy_matrimony_details) {
         var holyMatrimonyElements = document.querySelectorAll('#weddingevent .elementor-element-701b2bf7 h2');
         if (holyMatrimonyElements.length > 0) {
           holyMatrimonyElements[0].innerHTML = content.holy_matrimony_details.replace(/\n/g, '<br>');
         }
       }
       // Holy matrimony address
       if (content.holy_matrimony_address) {
         var holyMatrimonyAddressElements = document.querySelectorAll('#weddingevent .elementor-element-5a09c660');
         if (holyMatrimonyAddressElements.length > 0) {
           holyMatrimonyAddressElements[0].textContent = content.holy_matrimony_address;
         }
       }
       // Holy matrimony maps
       if (content.holy_matrimony_maps) {
         var holyMatrimonyMapsElements = document.querySelectorAll('#weddingevent .elementor-element-64d163cf a');
         if (holyMatrimonyMapsElements.length > 0) {
           holyMatrimonyMapsElements[0].href = content.holy_matrimony_maps;
         }
       }
       // Wedding reception details
       if (content.wedding_reception_details) {
         var receptionElements = document.querySelectorAll('#weddingevent .elementor-element-41ffd11c h2');
         if (receptionElements.length > 0) {
           receptionElements[0].innerHTML = content.wedding_reception_details.replace(/\n/g, '<br>');
         }
       }
       // Wedding reception address
       if (content.wedding_reception_address) {
         var receptionAddressElements = document.querySelectorAll('#weddingevent .elementor-element-6e088e65');
         if (receptionAddressElements.length > 0) {
           receptionAddressElements[0].textContent = content.wedding_reception_address;
         }
       }
       // Wedding reception maps
       if (content.wedding_reception_maps) {
         var receptionMapsElements = document.querySelectorAll('#weddingevent .elementor-element-704fe68c a');
         if (receptionMapsElements.length > 0) {
           receptionMapsElements[0].href = content.wedding_reception_maps;
         }
       }
       // Countdown image
       if (content.countdown_image) {
         var countdownImageElements = document.querySelectorAll('.countdown-image');
         if (countdownImageElements.length > 0) {
           countdownImageElements[0].style.backgroundImage = 'url(' + content.countdown_image + ')';
         }
       }
       // Countdown title
       if (content.countdown_title) {
         var countdownTitleElements = document.querySelectorAll('.countdown-title');
         countdownTitleElements.forEach(function(el) {
           el.textContent = content.countdown_title;
         });
       }
       // Save the date title
       if (content.save_the_date_title) {
         var saveDateTitleElements = document.querySelectorAll('.save-the-date-title');
         saveDateTitleElements.forEach(function(el) {
           el.textContent = content.save_the_date_title;
         });
       }
       // Save the date text
       if (content.save_the_date_text) {
         var saveDateTextElements = document.querySelectorAll('.save-the-date-text');
         saveDateTextElements.forEach(function(el) {
           el.textContent = content.save_the_date_text;
         });
       }
       
       // Background section 2 (groom)
       if (content.background_section_2) {
         var bgSection2 = document.querySelector('.background-section-2');
         if (bgSection2) {
           bgSection2.style.backgroundImage = 'url(' + content.background_section_2 + ')';
           bgSection2.style.backgroundSize = 'cover';
           bgSection2.style.backgroundPosition = 'center';
         }
       }
       // Background section 3 (bride)
       if (content.background_section_3) {
         var bgSection3 = document.querySelector('.background-section-3');
         if (bgSection3) {
           bgSection3.style.backgroundImage = 'url(' + content.background_section_3 + ')';
           bgSection3.style.backgroundSize = 'cover';
           bgSection3.style.backgroundPosition = 'center';
         }
       }
       
       // Hero section handling
       var heroSection = document.getElementById('hapus-section');
       if (heroSection) {
         // Add small delay for smooth transition
         setTimeout(function() {
           heroSection.style.opacity = '1';
           heroSection.style.visibility = 'visible';
           // Force override AOS animations for hero section only, preserve other animations
           if (heroSection.classList.contains('background-section-1')) {
             heroSection.classList.add('aos-animate');
             heroSection.classList.remove('aos-init');
           }
         }, 100);
       }
       
       // Refresh AOS after all content is loaded to ensure animations work properly
       setTimeout(function() {
         if (typeof AOS !== 'undefined') {
           AOS.refresh();
           // Double refresh after another delay to catch any late-loading elements
           setTimeout(function() {
             AOS.refresh();
             // Also refresh event sections after content loading
             setTimeout(function() {
               ensureAOSForEventSections();
             }, 200);
           }, 500);
         }
       }, 300);
     })
     .catch(function (err) {
       console.warn('Gagal memuat content.json:', err);
       // Still make hero section visible even if content loading fails (only for hero section)
       var heroSection = document.getElementById('hapus-section');
       if (heroSection) {
         setTimeout(function() {
           heroSection.style.opacity = '1';
           heroSection.style.visibility = 'visible';
           // Force override AOS animations for hero section only, preserve other animations
           if (heroSection.classList.contains('background-section-1')) {
             heroSection.classList.add('aos-animate');
             heroSection.classList.remove('aos-init');
           }
         }, 1000);
       }
     });
 });
